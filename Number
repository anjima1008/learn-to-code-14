//program to check whether the no is positive,negative,or zero

#include <stdio.h>

//Compiler version gcc  6.3.0

int main()
{
 int num;
 printf("Enter the no:\n");
 scanf("%d" , &num);
 if(num>0)
 {
   printf ("The number is positive \n");
 }
 else if(num<0)
    printf("The number is negative\n");
    else
    printf("The number is zero\n");
  
  return 0;
}
